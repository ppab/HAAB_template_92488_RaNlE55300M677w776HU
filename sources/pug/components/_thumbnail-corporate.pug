mixin thumbnail-corporate( obj )
	// Thumbnail Corporate
	if( obj.link )
		article.thumbnail-corporate.link&attributes( attributes )
			+img( obj.img ).thumbnail-corporate-image
			.thumbnail-corporate-caption
				p.thumbnail-corporate-title= obj.title
				if( obj.text )
					p= obj.text
				a.thumbnail-corporate-link( href= obj.link  data-lightgallery='item')
					+img( obj.img  )
					span.icon.mdi.mdi-plus
					span.icon.mdi.mdi-plus
			.thumbnail-corporate-dummy 
		
	if( obj.original )
		a.thumbnail-corporate( href= obj.original)&attributes( attributes )
			+img( obj.img ).thumbnail-corporate-image
			.thumbnail-corporate-caption
				p.thumbnail-corporate-title= obj.title
				if( obj.subtitle )
					p.thumbnail-corporate__subtitle= obj.subtitle
					
			.thumbnail-corporate-dummy 
