section.section.section-lg.section-last.text-center.bg-gray-100
	.container
		h3.wow-outer.text-center: span.wow.slideInDown Who We Are
		.row.row-50
			.col-sm-6.col-lg-4.wow-outer
				// Profile Minimal
				article.profile-minimal.wow.slideInLeft
					+img( 'team-1-370x368' ).profile-minimal-image
					.profile-minimal-caption
						h4.profile-minimal-title Jean Thompson
						p.profile-minimal-position Engraver

			.col-sm-6.col-lg-4.wow-outer
				// Profile Minimal
				article.profile-minimal.wow.slideInLeft( data-wow-delay='.05s' )
					+img( 'team-2-370x368' ).profile-minimal-image
					.profile-minimal-caption
						h4.profile-minimal-title John Doe
						p.profile-minimal-position Certified Engraver, Founder

			.col-sm-6.col-lg-4.wow-outer
				// Profile Minimal
				article.profile-minimal.wow.slideInLeft( data-wow-delay='.1s' )
					+img( 'team-3-370x368' ).profile-minimal-image
					.profile-minimal-caption
						h4.profile-minimal-title Nathan Porter
						p.profile-minimal-position Wood Engraver

		.wow-outer.button-outer
			a.button.button-lg.button-primary.button-winona.wow.slideInUp( href= links.about ) View all Team
